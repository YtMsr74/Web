@model Question

@{
    ViewData["Title"] = "Quiz";
}

<h1>@ViewData["Title"]</h1>
<form method="post">
    <div>@Model.eq</div>
    <input type="number" name="guess"/><span asp-validation-for="guess"></span> <br />
    <input type="submit" name="action" value="Next" class="btn btn-outline-dark mr-1 mt-3"/>
    <input type="submit" name="action" value="Finish" class="btn btn-primary mt-3"/>
</form>

@section Scripts{
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

